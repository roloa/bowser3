		//メイドのほうを向く
		var maiddist = maid._x - _x;
		if(maiddist < 0){
			_xscale = 100;	
		} else {
			_xscale = -100;
		}
		//ジャンプ中の動作
		if(0 < isJumpStandby){
			if(--isJumpStandby <= 0){
				//メイドの位置によってx速度が変わる
				vx = Math.min(Math.max(maiddist / 50,-8),8);
				vy -= 20;
				isJumping = true;
			}
		} else if(isJumping){
			//ブロックの境目の降下ポイントに近くて、十分な高度があり、メイドの真上に来たら
			if(_x % 100 < 30 || 70 < _x % 100 ){
				if(-10 < vy && -50 < maiddist && maiddist < 50){
					vx = 0;
					vy *= 0.2;
					isJumping = false;
					isDropping = true;
				}
			}
		} else if(isLanded){ //ジャンプ開始抽選
			if(jumpTimer < 0){
				vy = -10;
				isJumpStandby = 60;
				jumpTimer = 100;
			} else {
				jumpTimer--;
			}
		}
		
		
		//火を吐く
		if(isBreathing < 60){
		
			this.head._rotation = Math.sin(isBreathing / 60 * Math.PI) * 30;
			if(isBreathing == 45){
				var sindo = _parent.getNextHighestDepth();
				var flame = _parent.attachMovie(""flame"",""flame"" + sindo,sindo,
