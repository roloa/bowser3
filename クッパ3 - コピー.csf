			isBreathing = 0;
			this.head.gotoAndStop(2);
		}
	}
	var maidx = this._parent.maid._x * 0.5;
	var maidy = this._parent.maid._y * 0.5;
	//メイドを殺す当たり判定
	if(this.hitTest(maidx,maidy - 10) ||
	  this.hitTest(maidx,maidy - 30)){
	  this._parent.maid.damage();
	}

	

}

