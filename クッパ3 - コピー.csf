	if(1200 < _y){
	trace(_y);
		vy = -30;
		vx = -_xscale / 33;
	}
	isLanded = false;

	var bx = Math.floor((_x - 50) / 100);
	var by = Math.floor( (_y )/ 100 );
	//床の判定
	if(0 < vy && (_parent.blocks[bx][by] != null || _parent.blocks[bx+1][by] != null)){
		_y = by * 100;
		isLanded = true;
		_y = _parent.blockOffset;
		if(gravity *3 < vy){
			titi = vy;
			//ブラがずれる
			Lbreast.bra._yscale -= vy * Lbreast.bra._yscale * 0.00005;
			Rbreast.bra._yscale -= vy * Rbreast.bra._yscale * 0.00005;
		}
		vy = 0;
		vx = 0;
		isJumping = false;
		
		if(isDropping){
			_parent.quake();

			trace(bx + "","" + by);
			
			_parent.brake(bx,by);
			_parent.brake(bx+1,by);
			
			
			isDropping = false;
		}
	}
	


	
	this.Larm._rotation = -45+vy;
	this.Rarm._rotation = 180+45-vy;
	
	//乳揺れ
	if(0 < titi)titi -= 0.5;	
	Lbreast._x = Lbreast_offsetx +  Math.sin(titi) * titi * 0.1;
	Lbreast._y = Lbreast_offsety +  Math.cos(titi) * titi * 0.1;
	Rbreast._x = Rbreast_offsetx + -Math.sin(titi) * titi * 0.1;
	Rbreast._y = Rbreast_offsety +  Math.cos(titi) * titi * 0.1;

	//足の表示
	if(isDropping){
		//ドロップ中
		Lleg._visible = true;
