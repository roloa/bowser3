,"
function onLoad(){
	this._xscale = this._yscale = 0;
}

function onEnterFrame(){
	
	this._x += this.vx;
	this._y += this.vy;

	this._xscale = (this._yscale = this._yscale * 0.9 + 10);

	var bx = Math.floor((this._x) / 100);
	var by = Math.floor((this._y)/ 100 );
	if(this._parent.blocks[bx][by] != null){
			//ブロックに衝突すると横になる
		if(this.vy != 0){
			this._y -= this.vy;
			this.setSpeed(this.vx,0);
		} else {
			//横になってるときに衝突すると消える
			this.removeMovieClip();
		}
	}
	//trace(this._parent.maid._x);
	var maidx = this._parent.maid._x * 0.5;
	var maidy = this._parent.maid._y * 0.5;
	//メイドを殺す当たり判定
	if(this.hitTest(maidx,maidy - 10) ||
	  this.hitTest(maidx,maidy - 30)){
	  this._parent.maid.damage();
	}
	
}

function setSpeed(sx,sy){
	trace(sx +"":""+sy);
	this.vx = sx;
	this.vy = sy;
	this._rotation = Math.atan2(sy,sx) / Math.PI * 180;
	
	
}"
